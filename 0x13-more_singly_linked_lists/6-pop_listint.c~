#include <stdlib.h>
#include <string.h>
#include <stdio.h>
#include "lists.h"

int pop_listint(listint_t **head)
{
	int s = 0;
	struct listint_s *Newnode = malloc(sizeof(struct listint_s));
	
	if (*head == NULL)
		return (0);
	Newnode = *head;
	s = Newnode->n;
	*head = (*head)->next;
	free(Newnode);
	return(s);
}
