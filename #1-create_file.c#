#include "main.h"

/**
 * create_file - function that creates a file.
 * @filename: name of the file to be careated.
 * @text_content: a string to be written.
 * Return: integer.
 */

int create_file(const *filename, char *text_content)
{
	int fd;

	fd = open("filename", O_CREATE | O_WRONLY | O_TRUNC, 0600);

	if (fd == -1)
	{
		exit (1);
	}
	